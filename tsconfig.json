{
  "compilerOptions": {
    "target": "es2016",
    "module": "ESNext",
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "types": ["vite/client", "@histoire/plugin-vue/components", "element-plus/global", "@vueuse/core"]
  },
  "exclude": [".histoire", "node_modules", "public"]
}
